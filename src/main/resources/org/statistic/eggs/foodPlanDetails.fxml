<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.statistic.eggs.controller.FoodPlanController">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>

    <Label fx:id="foodPlanNameLabel" text="Food Plan Name" style="-fx-font-size: 16px; -fx-font-weight: bold;" />
    <Label fx:id="foodPlanDateLabel" text="Food Plan Name" style="-fx-font-size: 12px; -fx-font-weight: regular;" />

    <TextArea fx:id="feedDetailsTextArea" editable="false" wrapText="true" prefHeight="200" prefWidth="300"/>

    <Button text="Close" onAction="#closeWindow"/>
</VBox>
